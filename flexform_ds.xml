<T3DataStructure>
  <sheets>
	  <s_template>
		  <ROOT>
			<type>array</type>
			<el>
			
		  <showmode>
            <TCEforms>
              <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showmode</label>  
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array"> 
                    <numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showmode_standardview</numIndex> 
                    <numIndex index="1">1</numIndex>
                  </numIndex>
				  <numIndex index="1" type="array"> 
                    <numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showmode_singleview</numIndex> 
                    <numIndex index="1">2</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showmode_frontpageview</numIndex>
                    <numIndex index="1">3</numIndex>
                  </numIndex>
				  <numIndex index="3" type="array">
                    <numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showmode_contextcolumnview</numIndex>
                    <numIndex index="1">4</numIndex>
                  </numIndex>
                </items>   
                <maxitems>1</maxitems>
                <size>1</size>
              </config>
            </TCEforms>
          </showmode>
		  
		  <include_css>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.include_css</label>
	        <config>
	         <type>check</type>
			 <default>1</default>
	        </config>
	       </TCEforms>
	      </include_css>
		  
		  <template_file>
			<TCEforms>
				<label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.template_selection</label>
				<config>
					<type>group</type>
					<internal_type>file</internal_type>
					<allowed>tmpl,html,htm</allowed>

					
					<maxitems>1</maxitems>
					<size>1</size>
					<selectedListStyle>Width:180px</selectedListStyle>
				</config>
			</TCEforms>
		  </template_file>

	  
	      <step>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.numperpage</label>
	        <config>
	         <type>input</type>
	         <size>3</size>
	        </config>
	       </TCEforms>
	      </step>
		  
		  <csseven>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.csseven</label>
	        <config>
	         <type>input</type>
	         <size>30</size>
	        </config>
	       </TCEforms>
	      </csseven>
		  
		  <cssodd>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.cssodd</label>
	        <config>
	         <type>input</type>
	         <size>30</size>
	        </config>
	       </TCEforms>
	      </cssodd>
		  
		  <show_location>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.show_location</label>
	        <config>
	         <type>check</type>
	        </config>
	       </TCEforms>
	      </show_location>
		  

		  <show_time_mode>
			<TCEforms>
				<label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.show_time_mode</label>
				<config>
					<type>radio</type>
					<items type="array">
						<numIndex index="0" type="array">
							<numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.only_new_events</numIndex>
							<numIndex index="1">0</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.all_events</numIndex>
							<numIndex index="1">1</numIndex>
						</numIndex>
						<numIndex index="2" type="array">
							<numIndex index="0">LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.only_old_events</numIndex>
							<numIndex index="1">2</numIndex>
						</numIndex>
					</items>
				</config>
			</TCEforms>
		  </show_time_mode>	
		  
		  <show_morelink>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.show_morelink</label>
	        <config>
	         <type>check</type>
	        </config>
	       </TCEforms>
	      </show_morelink>
		  
		  <showTitleLink>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showTitleLink</label>
	        <config>
	         <type>check</type>
	        </config>
	       </TCEforms>
	      </showTitleLink>
		  
		  <showImages>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showImages</label>
	        <config>
	         <type>check</type>
			 <default>1</default>
	        </config>
	       </TCEforms>
	      </showImages>
		  
		  <showAllImages>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.showAllImages</label>
	        <config>
	         <type>check</type>
			 <default>1</default>
	        </config>
	       </TCEforms>
	      </showAllImages>
		  
		  			
		  
		  <morelink>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.morelink</label>
	        <config>
			<type>group</type>
			<internal_type>db</internal_type>
			<allowed>pages</allowed>
			<size>1</size>
			<maxitems>1</maxitems>
			<minitems>0</minitems>
			<show_thumbs>1</show_thumbs>
			</config>
	       </TCEforms>
	      </morelink>
		  
		  
		  <ascDesc>
	       <TCEforms>
	        <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.ascDesc</label>
	        <config>
	         <type>check</type>
	        </config>
	       </TCEforms>
	      </ascDesc>

		<sel_categories>
		  <TCEforms>
		   <label>LLL:EXT:kb_eventboard/locallang_tca.xml:kb_eventboard.categories</label>
		   <config>
			<type>select</type>
			<size>5</size>
			<multiple>0</multiple>
			<autoSizeMax>10</autoSizeMax>
			<minitems>0</minitems>
			<maxitems>99</maxitems>			
			<items type="int"></items>
			 <itemsProcFunc>tx_kbeventboard_addCategoriesToFlexForm->addCategories</itemsProcFunc>
		   </config>
		   </TCEforms>
		</sel_categories>			
			
			
			</el>
		  </ROOT>
		</s_template>
  </sheets>
</T3DataStructure>